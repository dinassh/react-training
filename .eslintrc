{
  "extends": ["react-app", "plugin:import/errors"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/forbid-prop-types": [
      "error",
      { "forbid": ["any", "array", "object"] }
    ],
    "react/prop-types": [
      "error",
      {
        "ignore": [],
        "customValidators": [],
        "skipUndeclared": false
      }
    ],
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["./src"]
      }
    }
  }
}
